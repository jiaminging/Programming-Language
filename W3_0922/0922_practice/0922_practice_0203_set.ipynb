    "#A,B 三種刑罰\n",
    "#A:有期徒刑、沒收、罰金\n",
    "#B:無期徒刑、罰金、褫奪公權\n",
    "\n",
    "#1\n",
    "setA = {\"有期徒刑\",\"沒收\",\"罰金\"}\n",
    "setB = {\"無期徒刑\",\"罰金\",\"褫奪公權\"}\n",
    "\n",
    "print(\"A:\",setA)\n",
    "print(\"B:\",setB)\n",
    "\n",
    "#2 B有A沒有\n",
    "print(\"B有A沒有:\",setB - setA)\n",
    "\n",
    "#3 共同刑罰\n",
    "print(setA & setB)\n",
    "print(\"罰金\" in setA)\n",
    "print(\"罰金\" in setB)\n",
    "print((setA & setB) in setA)"
